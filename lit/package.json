{
  "name": "sgrud-skeleton-lit",
  "private": true,
  "version": "0.0.0",
  "source": "./src/index.ts",
  "exports": "./dist/sgrud-skeleton-lit.modern.js",
  "unpkg": "./dist/sgrud-skeleton-lit.umd.js",
  "scripts": {
    "build": "sgrud construct --format modern,umd",
    "postbuild": "sgrud postbuild && sgrud runtimify",
    "start": "sgrud universal ./dist/index.html"
  },
  "amdNames": {
    "@babel/runtime/helpers/inheritsLoose": null,
    "@babel/runtime/helpers/taggedTemplateLiteralLoose": null,
    "lit": null,
    "lit/decorators.js": "lit",
    "sgrud-skeleton-lit": null,
    "tslib": "globalThis"
  },
  "sgrud": {
    "resources": [
      "./res/index.html"
    ],
    "runtimify": [
      "@babel/runtime:helpers.umd",
      "lit:!static.umd"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.21.5",
    "lit": "2.7.3",
    "tslib": "2.5.0"
  },
  "devDependencies": {
    "@sgrud/bin": "latest",
    "@types/css-modules": "1.0.2",
    "sass": "1.62.1"
  },
  "webDependencies": {
    "@babel/runtime": {
      "exports": {
        "@babel/runtime/helpers/": "./helpers/esm/"
      },
      "unpkg": [
        "./runtimify.umd.js"
      ]
    },
    "@lit/reactive-element": {
      "exports": {
        "@lit/reactive-element": "./reactive-element.js",
        "@lit/reactive-element/": "./"
      }
    },
    "lit": {
      "exports": {
        "lit": "./index.js",
        "lit/": "./"
      },
      "unpkg": [
        "./runtimify.umd.js"
      ]
    },
    "lit-element": {
      "exports": {
        "lit-element/": "./"
      }
    },
    "lit-html": {
      "exports": {
        "lit-html": "./lit-html.js",
        "lit-html/": "./"
      }
    },
    "regenerator-runtime": {
      "unpkg": [
        "./runtime.js"
      ]
    },
    "tslib": {
      "exports": {
        "tslib": "./tslib.es6.js"
      },
      "unpkg": [
        "./tslib.js"
      ]
    }
  }
}
